{
  "manifest_version": 2,
  "name": "test-detect-browser-api-object-in-content-script",
  "author": "Fake Author",
  "version": "0.1",
  "description": "test-detect-browser-api-object-in-content-script",
  "background": {
    "page": "background.html",
    "persistent": true
  },
